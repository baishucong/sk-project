<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sk.dao.EmployeeMapper">
    <select id="getEmp" resultType="com.sk.entity.Employee">
        select *from employee limit 5
    </select>
    <select id="getEmpById" resultType="com.sk.entity.Employee">
        select *from employee where id=#{id}
    </select>
</mapper>